<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <title>Portfolio Freelancer</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="Free HTML Templates" name="keywords">
    <meta content="Free HTML Templates" name="description">

    <!-- Favicon -->
    <link href="/img/favicon.ico" rel="icon">

    <!-- Google Web Fonts -->
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@300;400;500;700;900&display=swap" rel="stylesheet"> 

    <!-- Font Awesome -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">

    <!-- Libraries Stylesheet -->
    <link href="lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">
    <link href="lib/lightbox/css/lightbox.min.css" rel="stylesheet">

    <!-- Customized Bootstrap Stylesheet -->
    <link href="/css/style.css" rel="stylesheet">
</head>

<body data-spy="scroll" data-target=".navbar" data-offset="51">
    <!-- Navbar Start -->
    <nav class="navbar fixed-top shadow-sm navbar-expand-lg bg-light navbar-light py-3 py-lg-0 px-lg-5">
        <a href="#" class="navbar-brand ml-lg-3">
            <h1 class="m-0 display-5"><span class="<%= data.texto %>">PORTA</span>FOLIO</h1>
        </a>
        <button type="button" class="navbar-toggler" data-toggle="collapse" data-target="#navbarCollapse">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse px-lg-3" id="navbarCollapse">
            <div class="navbar-nav m-auto py-0">
                <a href="#home" class="nav-item nav-link active  ">Inicio</a>
                <a href="#about" class="nav-item nav-link">Sobre mí</a>
                <a href="#qualification" class="nav-item nav-link">Calidad</a>
                <a href="#skill" class="nav-item nav-link">Habilidades</a>
                <a href="#service" class="nav-item nav-link">Servicios</a>
                <a href="#portfolio" class="nav-item nav-link">Proyectos</a>
                <a href="#testimonial" class="nav-item nav-link">Comentarios</a>
                <a href="#blog" class="nav-item nav-link">Blog</a>
                <a href="#contact" class="nav-item nav-link">Contacto</a>
            </div>
            <a href="" class="btn <%= data.borde %> d-none d-lg-block">Contrátame</a>
        </div>
    </nav>
    <!-- Navbar End -->


    <!-- Video Modal Start -->
    <div class="modal fade" id="videoModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-body">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>        
                    <!-- 16:9 aspect ratio -->
                    <div class="embed-responsive embed-responsive-16by9">
                        <iframe class="embed-responsive-item" src="" id="video"  allowscriptaccess="always" allow="autoplay"></iframe>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Video Modal End -->


    <!-- Header Start -->
    <div class="container-fluid <%= data.color %> d-flex align-items-center mb-5 py-5" id="home" style="min-height: 100vh;">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-lg-5 px-5 pl-lg-0 pb-5 pb-lg-0">
                    <img class="img-fluid w-100 rounded-circle shadow-sm" src="<%= data.image %>" alt="">
                </div>
                <div class="col-lg-7 text-center text-lg-left">
                    <h3 class="text-white font-weight-normal mb-3">Soy</h3>
                    <h1 class="display-3 text-uppercase <%= data.texto %> mb-2" style="-webkit-text-stroke: 2px #ffffff;"><%= data.nombre %>!</h1>
                    <h1 class="typed-text-output d-inline font-weight-lighter text-white"></h1>
                    <div class="typed-text d-none"><%= data.mensajeuno %>, <%= data.mensajedos %>, <%= data.mensajetres %></div>
                    <div class="d-flex align-items-center justify-content-center justify-content-lg-start pt-5">
                        <a href="" class="btn btn-outline-light mr-5">Descargar CV</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Header End -->


    <!-- About Start -->
    <div class="container-fluid py-5" id="about">
        <div class="container">
            <div class="position-relative d-flex align-items-center justify-content-center">
                <h1 class="display-1 text-uppercase text-white" style="-webkit-text-stroke: 1px #dee2e6;">Conóceme</h1>
                <h1 class="position-absolute text-uppercase <%= data.texto %>">Sobre Mí</h1>
            </div>
            <div class="row align-items-center">
                <div class="col-lg-5 pb-4 pb-lg-0">
                    <img class="img-fluid rounded w-100" src="<%= data.image %>" alt="">
                </div>
                <div class="col-lg-7">
                    <h3 class="mb-4"><%= data.especialidad %></h3>
                    <p><%= data.descripcion %></p>
                    <div class="row mb-3">
                        <div class="col-sm-6 py-2"><h6>Nombre: <span class="text-secondary"><%= data.nombre %></span></h6></div>
                        <div class="col-sm-6 py-2"><h6>Cumpleaños: <span class="text-secondary"><%= data.cumple%></span></h6></div>
                        <div class="col-sm-6 py-2"><h6>Grado: <span class="text-secondary"><%= data.grado %></span></h6></div>
                        <div class="col-sm-6 py-2"><h6>Experiencia: <span class="text-secondary"><%= data.exp %></span></h6></div>
                        <div class="col-sm-6 py-2"><h6>Teléfono: <span class="text-secondary"><%= data.telefono %></span></h6></div>
                        <div class="col-sm-6 py-2"><h6>Email: <span class="text-secondary"><%= data.email %></span></h6></div>
                        <div class="col-sm-6 py-2"><h6>Ubicación: <span class="text-secondary"><%= data.ubicacion %></span></h6></div>
                        <div class="col-sm-6 py-2"><h6>Freelance: <span class="text-secondary"><%= data.free %></span></h6></div>
                    </div>
                    <a href="" class="btn <%= data.borde %> mr-4">Contrátame</a>
                </div>
            </div>
        </div>
    </div>
    <!-- About End -->


    <!-- Qualification Start -->
    <div class="container-fluid py-5" id="qualification">
        <div class="container">
            <div class="position-relative d-flex align-items-center justify-content-center">
                <h1 class="display-1 text-uppercase text-white" style="-webkit-text-stroke: 1px #dee2e6;">Calidad</h1>
                <h1 class="position-absolute text-uppercase <%= data.texto %>">Educación & Experiencia</h1>
            </div>
            <div class="row align-items-center">
                <div class="col-lg-6">
                    <h3 class="mb-4">Mi Educación</h3>
                    <div class="border-left <%= data.linea %> pt-2 pl-4 ml-2">
                        <% if (data.educacion) { %>
                            <% Object.values(data.educacion).forEach((educacion, index) => { %>
                                <div class="position-relative mb-4">
                                    <i class="far fa-dot-circle <%= data.texto%> position-absolute" style="top: 2px; left: -32px;"></i>
                                    <h5 class="font-weight-bold mb-1"><%= educacion.titulo %></h5>
                                    <p class="mb-2"><strong><%= educacion.lugar %></strong> | <small><%= educacion.fecha %></small></p>
                                    <p><%= educacion.resumen %></p>
                                </div>
                            <% }) %>
                        <% } else { %>
                            <p>No hay información de educación disponible</p>
                        <% } %>
                    </div>
                </div>
                <div class="col-lg-6">
                    <h3 class="mb-4">Mi experiencia</h3>
                    <div class="border-left <%= data.linea %> pt-2 pl-4 ml-2">
                        <% if (data.experiencia) { %>
                            <% Object.values(data.experiencia).forEach((experiencia, index) => { %>
                                <div class="position-relative mb-4">
                                    <i class="far fa-dot-circle <%= data.texto %> position-absolute" style="top: 2px; left: -32px;"></i>
                                    <h5 class="font-weight-bold mb-1"><%= experiencia.titulo %></h5>
                                    <p class="mb-2"><strong><%= experiencia.lugar %></strong> | <small><%= experiencia.fecha %></small></p>
                                    <p><%= experiencia.resumen %></p>
                                </div>
                            <% }) %>
                        <% } else { %>
                            <p>No hay información de experiencia disponible</p>
                        <% } %>
                    </div>
                </div>
            </div>
            
        </div>
    </div>
    <!-- Qualification End -->


    <!-- Skill Start -->
    <div class="container-fluid py-5" id="skill">
        <div class="container">
            <div class="position-relative d-flex align-items-center justify-content-center">
                <h1 class="display-1 text-uppercase text-white" style="-webkit-text-stroke: 1px #dee2e6;">Habilidades</h1>
                <h1 class="position-absolute text-uppercase <%= data.texto %>">Mis Habilidades</h1>
            </div>
            <div class="row align-items-center">
                <div class="col-md-6">
                    <% if (data.habilidad) { %>
                        <% Object.values(data.habilidad).slice(0, 3).forEach((habilidad, index) => { %>
                            <div class="skill mb-4">
                                <div class="d-flex justify-content-between">
                                    <h6 class="font-weight-bold"><%= habilidad.nombre %></h6>
                                    <h6 class="font-weight-bold"><%= habilidad.valor %>%</h6>
                                </div>
                                <div class="progress">
                                    <div class="progress-bar <%= habilidad.color %>" " role="progressbar" aria-valuenow="<%= habilidad.valor %>" aria-valuemin="0" aria-valuemax="100"></div>
                                </div>
                            </div>
                        <% }) %>
                    <% } else { %>
                        <p>No hay información de habilidades disponible</p>
                    <% } %>
                </div>
                <div class="col-md-6">
                    <% if (data.habilidad) { %>
                        <% Object.values(data.habilidad).slice(3, 6).forEach((habilidad, index) => { %>
                            <div class="skill mb-4">
                                <div class="d-flex justify-content-between">
                                    <h6 class="font-weight-bold"><%= habilidad.nombre %></h6>
                                    <h6 class="font-weight-bold"><%= habilidad.valor %>%</h6>
                                </div>
                                <div class="progress">
                                    <div class="progress-bar <%= habilidad.color %>" role="progressbar" aria-valuenow="<%= habilidad.valor %>" aria-valuemin="0" aria-valuemax="100"></div>
                                </div>
                            </div>
                        <% }) %>
                    <% } else { %>
                        <p>No hay información de habilidades disponible</p>
                    <% } %>
                </div>
            </div>
            
        </div>
    </div>
    <!-- Skill End -->


    <!-- Services Start -->
    <div class="container-fluid pt-5" id="service">
        <div class="container">
            <div class="position-relative d-flex align-items-center justify-content-center">
                <h1 class="display-1 text-uppercase text-white" style="-webkit-text-stroke: 1px #dee2e6;">Servicio</h1>
                <h1 class="position-absolute text-uppercase <%= data.texto %>">Mis Servicios</h1>
            </div>
            <div class="row pb-3">
                <% if (data.servicio && Object.keys(data.servicio).length > 0) { %>
                    <% Object.values(data.servicio).forEach((servicio, index) => { %>
                        <div class="col-lg-4 col-md-6 text-center mb-5">
                            <div class="d-flex align-items-center justify-content-center mb-4">
                                <i class="<%= servicio.icono %> service-icon <%= data.color %> text-white mr-3"></i>
                                <h4 class="font-weight-bold m-0"><%= servicio.titulo %></h4>
                            </div>
                            <p><%= servicio.descripcion %></p>
                            <a class="border-bottom <%= data.linea %> <%= data.texto %> text-decoration-none" href="">Leer Más</a>
                        </div>
                    <% }) %>
                <% } else { %>
                    <div class="col text-center">
                        <p>No hay servicios disponibles en este momento.</p>
                    </div>
                <% } %>
            </div>
        </div>
        
        
    </div>
    <!-- Services End -->


    <!-- Portfolio Start -->
    <div class="container-fluid pt-5 pb-3" id="portfolio">
        <div class="container">
            <div class="position-relative d-flex align-items-center justify-content-center">
                <h1 class="display-1 text-uppercase text-white" style="-webkit-text-stroke: 1px #dee2e6;">Proyecto</h1>
                <h1 class="position-absolute text-uppercase <%= data.texto %>">Mi Portafolio</h1>
            </div>
            <div class="row">
                <div class="col-12 text-center mb-2">
                    <ul class="list-inline mb-4" id="portfolio-flters">
                        <li class="btn btn-sm <%= data.borde %> m-1 active" data-filter="*">Todo</li>
                        <li class="btn btn-sm <%= data.borde %> m-1" data-filter=".first">Diseño</li>
                        <li class="btn btn-sm <%= data.borde %> m-1" data-filter=".second">Desarrollo</li>
                        <li class="btn btn-sm <%= data.borde %> m-1" data-filter=".third">Marketing</li>
                    </ul>
                </div>
            </div>
            <% if (data.portafolio && Object.keys(data.portafolio).length > 0) { %>
                <div class="row portfolio-container">
                    <% Object.keys(data.portafolio).forEach((tipo) => { %>
                        <% Object.values(data.portafolio[tipo]).forEach((proyecto) => { %>
                            <div class="col-lg-4 col-md-6 mb-4 portfolio-item <%= proyecto.categoria %>">
                                <div class="position-relative overflow-hidden mb-2">
                                    <img class="img-fluid rounded w-100" src="<%= proyecto.img %>" alt="">
                                    <div class="portfolio-btn <%= data.color %> d-flex align-items-center justify-content-center">
                                        <a href="<%= proyecto.enlace %>" data-lightbox="portfolio">
                                            <i class="fa fa-plus text-white" style="font-size: 60px;"></i>
                                        </a>
                                    </div>
                                </div>
                            </div>
                        <% }) %>
                    <% }) %>
                </div>
            <% } else { %>
                <div class="row">
                    <div class="col text-center">
                        <p>No hay proyectos disponibles en este momento.</p>
                    </div>
                </div>
            <% } %>
        </div>
        
    </div>
    <!-- Portfolio End -->


    <!-- Blog Start -->
    <div class="container-fluid pt-5" id="blog">
        <div class="container">
            <div class="position-relative d-flex align-items-center justify-content-center">
                <h1 class="display-1 text-uppercase text-white" style="-webkit-text-stroke: 1px #dee2e6;">Novedades</h1>
                <h1 class="position-absolute text-uppercase <%= data.texto %>">Últimas Novedades</h1>
            </div>
            <div class="row">
                <% if (data.novedades && Object.keys(data.novedades).length > 0) { %>
                    <% Object.values(data.novedades).forEach((novedad) => { %>
                        <div class="col-lg-4 mb-5">
                            <div class="position-relative mb-4">
                                <img class="img-fluid rounded w-100" src="<%= novedad.img %>" alt="">
                                <div class="blog-date <%= data.color %>">
                                    <h4 class="font-weight-bold mb-n1 text-white"><%= novedad.dia %></h4>
                                    <small class="text-white text-uppercase"><%= novedad.mes %></small>
                                </div>
                            </div>
                            <h5 class="font-weight-medium mb-4"><%= novedad.titulo %></h5>
                            <a class="btn btn-sm <%= data.borde %> py-2" href="<%= novedad.enlace %>">Leer Más</a>
                        </div>
                    <% }) %>
                <% } else { %>
                    <div class="col text-center">
                        <p>No hay novedades disponibles en este momento.</p>
                    </div>
                <% } %>
            </div>
        </div>
        
    </div>
    <!-- Blog End -->


    <!-- Contact Start -->
    <div class="container-fluid py-5" id="contact">
        <div class="container">
            <div class="position-relative d-flex align-items-center justify-content-center">
                <h1 class="display-1 text-uppercase text-white" style="-webkit-text-stroke: 1px #dee2e6;">Contacto</h1>
                <h1 class="position-absolute text-uppercase <%= data.texto %>">Contáctame</h1>
            </div>
            <div class="row justify-content-center">
                <div class="col-lg-8">
                    <div class="contact-form text-center">
                        <div id="success"></div>
                        <form name="sentMessage" id="contactForm" novalidate="novalidate">
                            <div class="form-row">
                                <div class="control-group col-sm-6">
                                    <input type="text" class="form-control p-4" id="name" placeholder="Tu Name"
                                        required="required" data-validation-required-message="Por favor, ingresa tu nombre" />
                                    <p class="help-block text-danger"></p>
                                </div>
                                <div class="control-group col-sm-6">
                                    <input type="email" class="form-control p-4" id="email" placeholder="Tu Email"
                                        required="required" data-validation-required-message="Por favor, ingresa tu email" />
                                    <p class="help-block text-danger"></p>
                                </div>
                            </div>
                            <div class="control-group">
                                <input type="text" class="form-control p-4" id="subject" placeholder="Asunto"
                                    required="required" data-validation-required-message="Por favor, ingresa el asunto" />
                                <p class="help-block text-danger"></p>
                            </div>
                            <div class="control-group">
                                <textarea class="form-control py-3 px-4" rows="5" id="message" placeholder="Mensaje"
                                    required="required"
                                    data-validation-required-message="Por favor, ingresa tu mensaje"></textarea>
                                <p class="help-block text-danger"></p>
                            </div>
                            <div>
                                <button class="btn <%= data.borde %>" type="button" id="sendMessageButton" onclick="sendFormData()">Enviar</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Contact End -->


    <!-- Footer Start -->
    <div class="container-fluid <%= data.color %> text-white mt-5 py-5 px-sm-3 px-md-5">
        <div class="container text-center py-5">
            <div class="d-flex justify-content-center mb-4">
                <a class="btn btn-light btn-social mr-2" href="#"><i class="fab fa-twitter"></i></a>
                <a class="btn btn-light btn-social mr-2" href="#"><i class="fab fa-facebook-f"></i></a>
                <a class="btn btn-light btn-social mr-2" href="#"><i class="fab fa-linkedin-in"></i></a>
                <a class="btn btn-light btn-social" href="#"><i class="fab fa-instagram"></i></a>
            </div>
            <div class="d-flex justify-content-center mb-3">
                <a class="text-white" href="#">Privacy</a>
                <span class="px-3">|</span>
                <a class="text-white" href="#">Terms</a>
                <span class="px-3">|</span>
                <a class="text-white" href="#">FAQs</a>
                <span class="px-3">|</span>
                <a class="text-white" href="#">Help</a>
            </div>
            <p class="m-0">&copy; <a class="text-white font-weight-bold" href="#">PORTAFOLIO</a>. All Rights Reserved.
            </p>
        </div>
    </div>
    <!-- Footer End -->

    <!-- Scroll to Bottom -->
    <i class="fa fa-2x fa-angle-down text-white scroll-to-bottom"></i>

    <!-- Back to Top -->
    <a href="#" class="btn btn-outline-dark px-0 back-to-top"><i class="fa fa-angle-double-up"></i></a>


    <!-- JavaScript Libraries -->
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.bundle.min.js"></script>
    <script src="/lib/typed/typed.min.js"></script>
    <script src="/lib/easing/easing.min.js"></script>
    <script src="/lib/waypoints/waypoints.min.js"></script>
    <script src="/lib/owlcarousel/owl.carousel.min.js"></script>
    <script src="/lib/isotope/isotope.pkgd.min.js"></script>
    <script src="/lib/lightbox/js/lightbox.min.js"></script>

    <!-- Template Javascript -->
    <script src="/js/main.js"></script>

    <script>
        function sendFormData() {
            // Crear un objeto FormData con los datos del formulario
            const formData = new FormData(document.getElementById("contactForm"));
    
            // Enviar una solicitud POST al servidor
            fetch("/submit-form", {
                method: "POST",
                body: formData
            }).then(response => {
                // Redireccionar a la página deseada después de enviar el formulario
                window.location.href = "confirmacion.ejs";
            }).catch(error => {
                console.error("Error al enviar el formulario:", error);
            });
        }
    </script>
</body>

</html>